{
    "application": "{{ name }}",
    "description": "Create Security Group: {{ name }}",
    "job": [
        {
            "credentials": "{{ environment }}",
            "description": "Security group for {{ name }}",
            "detail": "",
            "name": "{{ name }}",
            "regions": [
                "{{ region }}"
            ],
            "securityGroupIngress": [
                {
                    "endPort": 8080,
                    "name": "sg_apps",
                    "startPort": 8080,
                    "type": "tcp"
                }
            ],
            "subnet": "none",
            "type": "upsertSecurityGroup",
            "user": "[anonymous]",
            "vpcId": "{{ vpc }}"
        }
    ]
}
