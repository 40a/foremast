{
  "name": "TestPipeline",
  "application": "{{ app }}",
  "stages": [
    {
      "requisiteStageRefIds": [],
      "refId": "1",
      "type": "wait",
      "name": "Wait",
      "waitTime": 20
    }
  ],
  "triggers": [
    {
      "enabled": true,
      "type": "jenkins",
      "master": "JenkinsCI",
      "job": "a_forrest_edge_spinnaker",
      "propertyFile": "jenkins_vars"
    }
  ],
  "limitConcurrent": true,
  "parallel": true,
  "notifications": [
    {
      "level": "pipeline",
      "when": [
        "pipeline.starting",
        "pipeline.complete",
        "pipeline.failed"
      ],
      "type": "slack",
      "address": "spinnaker"
    }
  ]
}
