{
    "app": {
        "app_description": null,
        "app_ssh_key": "{{ env }}_access",
        "email": null,
        "eureka_enabled": false,
        "instance_profile": "{{ profile }}",
        "instance_type": "t2.micro"
    },
    "asg": {
        "hc_type": "ELB",
        "max_inst": 3,
        "min_inst": 1,
        "subnet_purpose": "internal"
    },
    "elb": {
        "certificate": null,
        "i_port": 8080,
        "i_proto": "HTTP",
        "lb_port": 80,
        "lb_proto": "HTTP",
        "subnet_purpose": "internal",
        "target": "TCP:8080"
    },
    "qe": {
    },
    "regions": [
        "us-east-1"
    ],
    "security_group": {
        "description": "Auto-Gen SG for {{ app }}",
        "egress": "0.0.0.0/0",
        "ingress": {
            "sg_apps": [
                { "start_port": "80", "end_port": "80", "protocol": "tcp" },
                { "start_port": "8080", "end_port": "8080", "protocol": "tcp" }
            ]
        }
    }
}
